{
    "name": "webhook-action",
    "version": "2.3.0",
    "description": "Github Webhook Action",
    "main": "dist/main.js",
    "scripts": {
        "build": "tsc && ncc build --license licenses.txt",
        "format": "prettier --write '**/*.ts'",
        "format-check": "prettier --check '**/*.ts'",
        "lint": "eslint src/**/*.ts",
